  "scripts": {
    "predev": "echo 'Checking production dependencies only...' && npm audit --omit=dev || echo '\\nNote: Development dependencies may show vulnerabilities that are safe for local development (e.g. esbuild dev server).'",
    "dev": "vite dev --host",
    "build": "CF_PAGES=1 vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "USE_AUTO_ADAPTER=true vitest run",
    "test:watch": "USE_AUTO_ADAPTER=true vitest watch",
    "lint": "eslint . && svelte-check --tsconfig ./tsconfig.json",
    "lint:fix": "eslint . --fix",
    "check:structure": "svelte-kit sync && eslint src/routes/+page.svelte src/routes/+layout.svelte --config .eslintrc.cjs"
  },
  "overrides": {
    "glob": "^10.3.10",
    "rimraf": "^5.0.5",
    "@rollup/plugin-inject": "^5.0.5",
    "@jridgewell/sourcemap-codec": "^1.4.15",
